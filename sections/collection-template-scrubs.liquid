<!-- collection-template-scrubs.liquid -->
<script
  src="https://code.jquery.com/jquery-3.6.0.min.js"
  integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4="
  crossorigin="anonymous"></script>
<style>
  .grid-product__colors {
    display: none !important;
  }
  .hidden{
    display:none !important;
  }

</style>
<!-- template-scrubs -->
{% if collection.handle == "compression-socks" %}
  <script>
    $(document).ready(function(){
      var currentPageURL = window.location.href;
    
    // Check if the current page URL contains "fbclid"
    if(currentPageURL.indexOf("fbclid") !== -1) {
      window.location.replace("/pages/6-reasons-why-aussie-nurses-are-choosing-dr-woof-compression-socks");        
      // You can perform further actions here if the link is from Facebook
    } else {
        console.log("The link to this page is not from Facebook.");
        // You can perform further actions here if the link is not from Facebook
    }
    })
    
  </script>
{% endif %}
{% assign product_count = collection.products_count %}
<div class="page-width">
  <span style="display:block;margin-bottom:12px;text-align:right;text-decoration:underline;font-weight:800;width:100%">{{ product_count }} Total</span>
</div>
<div class="page-width">
  <div id="CollectionAjaxContent">
    <div class="grid grid--uniform grid--scattered-large-3 grid--scattered-small-2" data-collection-container="">

      {% capture first_mf %}
        {% assign first_image = collection.metafields.sf_collection.first_image %}
        {% assign first_text = collection.metafields.sf_collection.first_text %}
        {% assign first_url = collection.metafields.sf_collection.first_url %}

        {% if first_image != blank or first_text != blank or first_url != blank %}

      <div class="grid__item grid-product 1_ nohid small--one-half medium-up--one-third main-metafield-image {% if product.tags contains "hidden" %}hidden{% endif %}" >
            <div class="Main-alt-image grid__image-ratio grid__image-ratio--portrait lazyloaded">
              <div class="metafield-image collection-image-metafield">
                {% if first_image != blank %}
                  {% if first_url != blank %}
                    <a href="{{ first_url }}">
                    {% endif %}
                    <img
                      src="{{ first_image }}"
                      class="image-metafield-one"
                      alt="{% if first_text != blank %}{{ first_text }}{% else %}{{ collection.title }}{% endif %}" />
                    {% if first_url != blank %}
                    </a>
                  {% endif %}
                {% endif %}
              </div>
              <div class="metafield-text-url">
                {% if first_url != blank %}
                  <a href="{{ first_url }}">
                  {% endif %}
          
                  <div class="grid-product__content custom_collection_product__content">
                    <div class="grid-product__meta">
                      <div class="grid-product__title">
                        {{ first_text }}
                      </div>
                    </div>
                  </div>
                  {% if first_url != blank %}
                  </a>
                {% endif %}
              </div>
            </div>
          </div>
        {% endif %}
      {% endcapture %}
      {% capture second_mf %}
        {% assign second_image = collection.metafields.sf_collection.second_image %}
        {% assign second_title = collection.metafields.sf_collection.second_title %}
        {% assign second_url = collection.metafields.sf_collection.second_url %}
        {% if second_image != blank or second_title != blank or second_url != blank %}
          <div class="grid__item grid-product 1_ nohid small--one-half medium-up--one-third main-metafield-image {% if product.tags contains "hidden" %}hidden{% endif %}" >
            <div class="Main-alt-image grid__image-ratio grid__image-ratio--portrait lazyloaded">
              <div class="metafield-image collection-image-metafield">
                {% if second_image != blank %}
                  {% if second_url != blank %}
                    <a href="{{ second_url }}">
                    {% endif %}
                    <img
                      src="{{ second_image }}"
                      class="image-metafield-one"
                      alt="{% if second_title != blank %}{{ second_title }}{% else %}{{ collection.title }}{% endif %}" />
                    {% if second_url != blank %}
                    </a>
                  {% endif %}
                {% endif %}
              </div>
              <div class="metafield-text-url">
                {% if second_title != blank %}
                  {% if second_url != blank %}
                    <a href="{{ second_url }}">
                    {% endif %}
                    <div class="grid-product__content custom_collection_product__content">
                      <div class="grid-product__meta">
                        <div class="grid-product__title">
                          {{ second_title }}
                        </div>
                      </div>
                    </div>
                    {% if second_url != blank %}
                    </a>
                  {% endif %}
                {% endif %}
              </div>
            </div>
          </div>
        {% endif %}
      {% endcapture %}
      {% comment %}
      This sections requires the same `paginate_by` variable value as
      the other sections for the ajax loading to work.
      If you change the number below, it must also be changed in:
      - sections/collection-template
      - sections/featured-collection-slider
      - snippets/page-block-featured-collection-slider
      {% endcomment %}
      {%- assign paginate_by = 500 -%}



      {% if section.settings.collection_tags_style == 'inline' and collection.all_tags.size > 0 %}
        {%- assign paginate_by = paginate_by | minus: 1 -%}
      {% endif %}

      {% case section.settings.per_row_desktop %}
        {% when 2 %}
        {%- assign grid_large_width = 'medium-up--one-half' -%}
        {% when 3 %}
        {%- assign grid_large_width = 'medium-up--one-third' -%}
        {% when 4 %}
        {%- assign grid_large_width = 'medium-up--one-quarter' -%}
      {% endcase %}
      {%- assign per_row_mobile = section.settings.per_row_mobile -%}
      {%- assign grid_small_width = 'grid-product--padded' -%}
      {%- if section.settings.per_row_mobile contains '2' -%}
        {%- assign per_row_mobile = 2 -%}
        {%- assign grid_small_width = 'small--one-half' -%}
      {%- endif -%}

      {%- capture grid_item_width -%}
        {{ grid_small_width }} {{ grid_large_width }}{%- endcapture -%}

      {% paginate collection.products by 1000 %}
        {% assign metafield = 1 %}
          {% for product in collection.products limit: 1000 %}
          {% unless template contains 'index' %}
            {% for product_option in product.options_with_values %}
              {% assign option_name = product_option.name | handle %}
              {%- if option_name == 'color' or option_name == "Color" -%}
                {%- assign option_values = product_option.values | join: ',' | downcase | split: ',' -%}
                {% assign color_swatch = collection.metafields.sf_variant_swatch.color_option %}
                {% assign my_array = '' %}
                {% assign options = '' %}
                {% for value in color_swatch %}
                  {% assign tagname = value | split: "," %}
                  {% assign first = tagname | downcase %}
                  {% assign new_value = tagname %}
                  {% assign my_array = my_array | append: ',' | append: new_value %}
                {% endfor %}
                {% assign newtag = my_array | uniq | join: "," %}
                {% assign options = my_array | split: ',' %}
                {% for option in options %}
                  {% assign finall = option | remove: '"' | remove: "[" | remove: "]" | downcase %}
                  {%- for option_value in option_values -%}
                    {% assign main_option = finall | strip %}
                    {% assign finall_option_value = option_value | strip %}
                    {% if main_option != blank %}
                      {% if main_option == finall_option_value %}
                        {% assign forloop_lenght_o_v = forloop.length %}
                        {% if forloop.length != 1 %}

                          {%- assign featured_image = custom_variant.featured_image -%}
                          {%- assign product_url = custom_variant.url -%}

                          {% for variant in product.variants %}
                            {% assign messagetager = false %}
                            {% assign var_title_temp = variant.title | downcase %}
                            {% assign var_title_temp0 = var_title_temp | split: ' / ' %}

                            {% if var_title_temp0 contains option_value %}
                              {% assign coll_prod_var_sp = var_title_temp0 %}

                              {% assign metafield = metafield | plus: 1 %}

                              <div
                                class="grid__item grid-product 1_{{ sold_out }} {% if product.tags contains 'hidden' %}hidden{% else %}nohid{% endif %} {{ grid_item_width }}{% if settings.product_hover_details %} grid-product__hover-details{% endif %} {% if coll_prod_var_sp[1] %}{{coll_prod_var_sp[1] | handleize }}{% endif %} cc"
                                data-aos
                                data-product-grid
                                data-id="{{ product.id }}">
                                <div class="grid-product__content">
                                  {% if has_custom_label %}
                                    <div class="grid-product__tag grid-product__tag--custom">
                                      {{ custom_label }}
                                    </div>
                                  {% else %}
                                  {% comment %}
                                    {% if sold_out %}
                                      <div class="grid-product__tag grid-product__tag--sold-out">
                                        {{ 'products.product.sold_out' | t }}
                                      </div>
                                  {% endif %}
                                {% endcomment %}

                                    {% if variant.compare_at_price > variant.price %}
                                      <div class="grid-product__tag grid-product__tag--sale">
                                        {% if settings.product_save_amount %}
                                          {% if settings.product_save_type == 'dollar' %}
                                            {% capture saved_amount %}
                                              {{ product.compare_at_price | minus: product.price | money }}{% endcapture %}
                                          {% else %}
                                            {% capture saved_amount %}
                                              {{ product.compare_at_price | minus: product.price | times: 100.0 | divided_by: product.compare_at_price | round }}%{% endcapture %}
                                          {% endif %}
                                          {{ 'products.general.save_html' | t: saved_amount: saved_amount }}
                                        {% else %}
                                          {{ 'products.general.sale' | t }}
                                        {% endif %}
                                      </div>
                                    {% endif %}
                                  {% endif %}

                                  {%- assign fixed_aspect_ratio = false -%}
                                  {% unless settings.product_grid_image_size == 'natural' %}
                                    {%- assign fixed_aspect_ratio = true -%}
                                  {% endunless %}
                                  {% if variant.featured_image != blank %}

                                    {%- assign preview_image = variant.featured_image -%}
                                    {%- if force_jpg -%}
                                      {%- assign img_url = variant.featured_image | img_url: '1x1', format: 'jpg' | replace: '_1x1.', '_{width}x.' -%}
                                    {%- else -%}
                                      {%- assign img_url = variant.featured_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
                                    {%- endif -%}

                                  {% else %}
                                    {%- assign preview_image = product.featured_media.preview_image -%}
                                    {%- if force_jpg -%}
                                      {%- assign img_url = preview_image | img_url: '1x1', format: 'jpg' | replace: '_1x1.', '_{width}x.' -%}
                                    {%- else -%}
                                      {%- assign img_url = preview_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
                                    {%- endif -%}


                                  {%- endif -%}


                                  {%- assign image_limit = 1 -%}
                                  {%- assign image_count = 0 -%}
                                  {% if settings.product_hover_image %}
                                    {%- assign image_limit = 10 -%}
                                  {% endif %}

                                  {%- assign slider_image_padding = '100%' -%}
                                  {% if coll_prod_var_sp[1] %}
                                    {% assign accolor = coll_prod_var_sp[1] %}
                                  {% endif %}
                                  {% for tag in product.tags %}

                                    {% if tag contains "Callout" or tag contains "CallOut" %}
                                      {% assign collectiontag = tag | split: " - " %}
                                      {% assign colortag = collectiontag[1] %}
                                      {% assign messagetag = collectiontag[2] %}
                                      {% assign colortag = colortag | handleize %}
                                      {% assign accolor = accolor | handleize %}
                                      {% if colortag == accolor %}
                                        {% assign messagetager = messagetag %}
                                      {% else %}
                                        {% if product.type == "Printed Pattern Scrub Tops" or product.type == "Compression Socks" %}
                                          {% assign messagetager = messagetag %}
                                        {% endif %}
                                      {% endif %}
                                    {% endif %}
                                  {% endfor %}

                                  <a
                                    href="{{ product.url }}?variant={{variant.id}}"
                                    class="grid-product__link{% if sold_out %} grid-product__link--disabled{% endif %} {% if enable_quick_shop %} js-no-transition{% endif %}"
                                    data-product-id="{{ product.id }}"
                                    data-url="{{ product.url }}">

                                    <div class="image-quick" style="position:relative">
                                      {% assign my_value = variant.metafields.sf_variantsglobal.product_tag %}

                                      {% if messagetager %}
                                        <span class="collection-variant-tag">
                                          {{ messagetager }}
                                        </span>
                                      {% endif %}

                                      <div
                                        class="product-slider"
                                        data-image-count="{{ product.media.size }}"
                                        data-id="{{ product.id }}">
                                        {%- assign image_count = image_count | plus: 1 -%}
                                        <div class="product-slide">
                                          <div class="image-wrap">
                                            {% if forloop.first %}
                                              {% if fixed_aspect_ratio %}
                                                <div
                                                  class="Main-alt-image grid__image-ratio grid__image-ratio--{{ settings.product_grid_image_size }} lazyload"
                                                  style="background-image: url({{ preview_image | img_url:'grande' }});"
                                                  data-bgset="{% include 'bgset', image: preview_image %}"
                                                  data-sizes="auto"></div>


                                                {% assign imagealtvalue = 0 %}
                                                {% for image in product.images %}
                                                  {% assign down_alt = image.alt | strip %}
                                                  {% assign fullalt = down_alt | split: '[' | last %}
                                                  {% assign FINAL_fullalt = fullalt | split: ']' | first %}
                                                  {% assign img_alt_crnt_0 = FINAL_fullalt | downcase %}
                                                  {% if img_alt_crnt_0 == coll_prod_var_sp[1] %}
                                                    {% unless preview_image == image.src %}
                                                      <div
                                                        class="secondary-alt-image grid__image-ratio grid__image-ratio--{{ settings.product_grid_image_size }} lazyload"
                                                        style="background-image: url({{ image.src | img_url:'grande' }});"
                                                        data-bgset="{% include 'bgset', image: image.src %}"
                                                        data-sizes="auto"></div>
                                                    {% endunless %}
                                                    {% assign imagealtvalue = imagealtvalue | plus: 1 %}
                                                  {% endif %}
                                                {% endfor %}
                                                {% if imagealtvalue == 0 %}
                                                  {%- assign imagealtvalue_image = product.featured_media.preview_image -%}
                                                  <div
                                                    class="secondary-alt-image grid__image-ratio grid__image-ratio--{{ settings.product_grid_image_size }} lazyload"
                                                    style="background-image: url({{ imagealtvalue_image | img_url:'grande' }});"
                                                    data-bgset="{% include 'bgset', image: imagealtvalue_image %}"
                                                    data-sizes="auto"></div>

                                                {% endif %}



                                              {% else %}
                                                {%- assign slider_image_padding = 100 | divided_by: preview_image.aspect_ratio | append: '%' -%}
                                                <div class="image-wrap" style="height: 0; padding-bottom: {{ slider_image_padding }};">
                                                  <img
                                                    data-swift-lazy="1"
                                                    loading="lazy"
                                                    class="grid-product__image lazyload"
                                                    data-src="{{ img_url }}"
                                                    data-widths="[750, 900, 1080]"
                                                    data-aspectratio="{{ preview_image.aspect_ratio }}"
                                                    data-sizes="auto"
                                                    alt="{{ preview_image.alt | escape }}">
                                                  <noscript>
                                                    <img
                                                      data-swift-lazy="1"
                                                      loading="lazy"
                                                      class="grid-product__image lazyloaded"
                                                      src="{{ preview_image | img_url: '400x' }}"
                                                      alt="{{ preview_image.alt | escape }}">
                                                  </noscript>
                                                </div>
                                              {% endif %}
                                            {% else %}
                                              <div
                                                class="Main-alt-image grid__image-ratio grid__image-ratio--{{ settings.product_grid_image_size }} lazyload"
                                                {% if settings.product_grid_image_size == 'natural' %}
                                                style="padding-bottom: {{ slider_image_padding }}"
                                                {% endif %}
                                                style="background-image: url({{ preview_image | img_url:'grande' }});"
                                                data-sizes="auto"></div>
                                              {% assign imagealtvalue = 0 %}
                                              {% for image in product.images %}
                                                {% assign down_alt = image.alt | strip %}
                                                {% assign fullalt = down_alt | split: '[' | last %}
                                                {% assign FINAL_fullalt = fullalt | split: ']' | first %}
                                                {% assign img_alt_crnt_0 = FINAL_fullalt | downcase %}

                                                {% if img_alt_crnt_0 == coll_prod_var_sp[1] %}
                                                  {% unless preview_image == image.src %}
                                                    <div
                                                      class="secondary-alt-image grid__image-ratio grid__image-ratio--{{ settings.product_grid_image_size }} lazyload"
                                                      style="background-image: url({{ image.src | img_url:'grande' }});"
                                                      data-bgset="{% include 'bgset', image: image.src %}"
                                                      data-sizes="auto"></div>
                                                  {% endunless %}
                                                  {% assign imagealtvalue = imagealtvalue | plus: 1 %}
                                                {% endif %}
                                              {% endfor %}
                                              {% if imagealtvalue == 0 %}
                                                {%- assign imagealtvalue_image = product.featured_media.preview_image -%}
                                                <div
                                                  class="secondary-alt-image grid__image-ratio grid__image-ratio--{{ settings.product_grid_image_size }} lazyload"
                                                  style="background-image: url({{ imagealtvalue_image | img_url:'grande' }});"
                                                  data-bgset="{% include 'bgset', image: imagealtvalue_image %}"
                                                  data-sizes="auto"></div>

                                              {% endif %}
                                            {% endif %}
                                          </div>
                                        </div>
                                      </div>
                                      {% comment %}
                                      {% unless product.type == "Bundle" %}
                                      <a class="Custom_Quick_View grid-product__link quick-product__btn js-screen-open-product-{{ product.id }} js-no-transition"
                                      data-product-id="{{ product.id }}"
                                      data-url="{{ product.url | within: collection }}?variant={{variant.id}}">
                                      QUICK BUY
                                      </a>
                                      {% endunless %}
                                      {% endcomment %}
                                    </div>
                                    {% if image_count > 1 %}
                                      {%- assign loader_duration = 1350 | times: image_count -%}
                                      <style>
                                        .product-slider.slick-initialized[data-id="{{ product.id }}"]:after {
                                          animation-duration: {{ loader_duration }}
                                          ms;
                                        }
                                      </style>
                                    {% endif %}

                                    <div class="grid-product__meta">
                                      {% if section.settings.pre-navy %}
                              {% unless product.title contains "Women" %}
                                {% if product.title contains "Friendly" %}
                                  <img src="https://cdn.shopify.com/s/files/1/0573/7250/8344/files/friendly_2.svg?v=1707276467" class="dv fullimage"/>
                                  <img src="https://cdn.shopify.com/s/files/1/0573/7250/8344/files/friendly_2.svg?v=1707276467" style="" class="mv fullimage"/>
                                {% else %}
                                  <img src="https://cdn.shopify.com/s/files/1/0573/7250/8344/files/jogmen.svg?v=1709833507" class="dv fullimage"/>
                                  <img src="https://cdn.shopify.com/s/files/1/0573/7250/8344/files/jogmen.svg?v=1709833507" style="" class="mv fullimage"/>
                                {% endif %}
                              {% else %}

                                {% if product.title contains "Friendly" %}
                                  <img src="https://cdn.shopify.com/s/files/1/0573/7250/8344/files/friendly_2.svg?v=1707276467" class="dv fullimage"/>
                                  <img src="https://cdn.shopify.com/s/files/1/0573/7250/8344/files/friendly_2.svg?v=1707276467" style="" class="mv fullimage"/>
                                {% else %}
                                  {% if product.title contains "Eco-Soft" %}
                                    <img src="https://cdn.shopify.com/s/files/1/0573/7250/8344/files/eco.svg?v=1707232976" class="dv fullimage"/>
                                    <img src="https://cdn.shopify.com/s/files/1/0573/7250/8344/files/eco.svg?v=1707232976" style="" class="mv fullimage"/>
                                  {% else %}
                                    {% if product.title contains "Skinny" or product.title contains "Straight" %}
                                      <img src="https://cdn.shopify.com/s/files/1/0573/7250/8344/files/straight.svg?v=1707275880" class="dv fullimage"/>
                                      <img src="https://cdn.shopify.com/s/files/1/0573/7250/8344/files/straight.svg?v=1707275880" style="" class="mv fullimage"/>
                                    {% else %}
                                    {% if product.title contains "V-Neck" %}
                                      <img src="https://cdn.shopify.com/s/files/1/0573/7250/8344/files/vneck.png?v=1710139882" class="dv fullimage"/>
                                      <img src="https://cdn.shopify.com/s/files/1/0573/7250/8344/files/vneck.png?v=1710139882" style="" class="mv fullimage"/>
                                    {% else %}
                                      <img src="https://cdn.shopify.com/s/files/1/0573/7250/8344/files/jog.svg?v=1707275864" class="dv fullimage"/>
                                      <img src="https://cdn.shopify.com/s/files/1/0573/7250/8344/files/jog.svg?v=1707275864" style="" class="mv fullimage"/>
                                    {% endif %}
                                    {% endif %}
                                  {% endif %}
                                {% endif %}
                              {% endunless %}


                            {% endif %}
                                      <div class="grid-product__title">{{ product.title }}</div>

                                      {% if coll_prod_var_sp[1] %}
                                        <span class="product__title_color_variants" var-name="{{ coll_prod_var_sp[1] | handleize }}">
                                          {{ coll_prod_var_sp[1] | capitalize }}
                                        </span>
                                      {% endif %}
                                      {% if settings.enable_product_loox_reviews and settings.enable_product_loox_reviews_collection %}{% comment %} this block was created by belowyellow.com {% endcomment %}{% render 'loox_reviews', product: product %}{% endif %}

                                      {% if settings.vendor_enable %}
                                        <div class="product-single__vendor">
                                          {%- assign vendor_handle = product.vendor | handleize -%}
                                          {%- if collections[vendor_handle] != empty -%}
                                            <a href="{{ routes.collections_url }}/{{ collections[vendor_handle].handle }}">
                                              {{ collections[vendor_handle].title }}
                                            </a>
                                          {%- else -%}
                                            {{ product.vendor | link_to_vendor }}
                                          {%- endif -%}
                                        </div>
                                      {% endif %}

                                      <div class="grid-product__price 111">
                                        {% assign normal_price = product.metafields.sf_product_bundle.normal_price %}
                                        {% assign sf_product_bundlecompare_price = product.metafields.sf_product_bundle.compare_price %}

                                        {% if normal_price != blank and sf_product_bundlecompare_price != blank %}
                                          <span class="visually-hidden">{{ 'products.general.regular_price' | t }}</span>
                                          <del class="grid-product__price--original">${{ sf_product_bundlecompare_price }}</del>
                                          <span class="visually-hidden">{{ 'products.general.sale_price' | t }}</span>

                                          <span {% if on_sale %}class="sale-price"{% endif %}>
                                            ${{ normal_price }}
                                          </span>

                                        {% else %}
                                          {% if normal_price != blank %}
                                            <span {% if on_sale %}class="sale-price"{% endif %}>
                                              ${{ normal_price }}
                                            </span>
                                          {% endif %}

                                        {% endif %}

                                        {% unless normal_price != blank and sf_product_bundlecompare_price != blank %}

                                          {% if variant.compare_at_price > variant.price %}
                                            <span class="visually-hidden">{{ 'products.general.regular_price' | t }}</span>
                                            <del class="grid-product__price--original">{{ product.compare_at_price | money }}</del>
                                            <span class="visually-hidden">{{ 'products.general.sale_price' | t }}</span>
                                          {% endif %}
                                          {% if product.price_varies %}
                                            <!--{% assign price = product.price_min | money %}
                                            <span {% if on_sale %}class="sale-price"{% endif %}>
                                            {{ 'products.general.from_text_html' | t: price: price }}
                                            </span>-->
                                            <!-- commented by mlveda -->
                                              <span {% if variant.compare_at_price > variant.price %}class="classa" style="color:red"{% endif %}> {{ variant.price | money }}
                                            </span>
                                          {% else %}

                                            <span {% if variant.compare_at_price > variant.price %}class="classb sale-price"{% endif %}>
                                              {{ product.price | money }}
                                            </span>
                                          {% endif %}
                                        {% endunless %}



                                        <div style="display:none;" class="all-images-here">
                                          {% for image in product.images %}
                                            {% assign down_alt = image.alt | strip %}
                                            {% assign fullalt = down_alt | split: '[' | last %}
                                            {% assign FINAL_fullalt = fullalt | split: ']' | first %}
                                            {% assign img_alt_crnt_0 = FINAL_fullalt | downcase %}

                                          {% endfor %}
                                        </div>

                                        {% comment %}
                                        <div class="all-images-with-swatchs">
                                        {% assign Variant_imagealtvalue = 0 %}
                                        {% for image in product.images %}
                                        {% assign down_alt = image.alt | strip %}
                                        {% assign fullalt = down_alt | split:'[' | last %}
