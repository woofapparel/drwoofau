    function findVariantId(productHandle, productType, productSize) {
    var product = productsData[productHandle];
    
    if (product) {
      var variants = product.variants;
      for (var i = 0; i < variants.length; i++) {
        var variant = variants[i];
        if (variant.option1 === productType && variant.option2 === productSize) {
          return variant.id; // Return the variant ID that matches both options
        }
      }
    }
     
    return null; // Return null if no matching variant is found
  }
  function findVariant(productHandle, productType, productSize) {
    var product = productsData[productHandle];
    
    if (product) {
      var variants = product.variants;
      for (var i = 0; i < variants.length; i++) {
        var variant = variants[i];
        if (variant.option1 === productType && variant.option2 === productSize) {
          return variant; // Return the entire variant object
        }
      }
    }
    
    return null; // Return null if no matching variant is found
  }

  // Function to check if a variant is available
  function isVariantAvailable(productHandle, productType, productSize) {
    var variant = findVariant(productHandle, productType, productSize);
    
    if (variant) {
      return variant.available; // Returns true or false based on availability
    }

    return false; // Return false if the variant is not found
  }

  // Function to get the stock quantity of the variant
  function getVariantStock(productHandle, productType, productSize) {
    var variant = findVariant(productHandle, productType, productSize);
    
    if (variant) {
      return variant.inventory_quantity; // Returns the stock quantity
    }

    return 0; // Return 0 if the variant is not found
  }

