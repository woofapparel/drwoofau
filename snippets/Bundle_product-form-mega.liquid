{% if intro %}
  <div>
    <p>{{ intro }}</p>
  </div>
{% endif %}
{% capture size_chart %}
  {% for titletag in product.tags %}
    {% assign titleHere = titletag | downcase | strip %}
    {% if titleHere == 'sizechart_men' %}
      <div class="Main_size_chart_custom">
        <a
          class="size_chart_custom"
          target="_BLANK"
          href="https://www.drwoofapparel.com.au/pages/sizing-men">Size chart</a>
      </div>
    {% endif %}
    {% if titleHere == 'sizechart_women' %}
      <div class="Main_size_chart_custom">
        <a
          class="size_chart_custom"
          target="_BLANK"
          href="https://www.drwoofapparel.com.au/pages/sizing-female">Size chart</a>
      </div>
    {% endif %}
  {% endfor %}

{% endcapture %}


{% if template contains 'Bundle_Metafield' or template contains 'new-bundle-template' %}

  {% capture error_msg %}
    <div class="error_massage_Select">Please select Available Variants</div>
  {% endcapture %}
  <div class="Main_product_variants">

    {% unless product.has_only_default_variant %}
      <div class="Simple_variants">
        {% for option in product.options_with_values %}
          {% if variant_type == 'button' %}
            {% include 'variant-button', section_id: section_id, option_drop: option %}
          {% else %}
            {% include 'variant-dropdown', section_id: section_id %}
          {% endif %}
        {% endfor %}
      </div>
    {% endunless %}
    <div class="product_One_Shown">
      <div class="custom_builder_option">
        {{ error_msg }}
        <select
          name="id"
          id="ProductSelect-{{ section_id }}"
          class="product-single__variants no-js"
          data-product-select>
          {% for variant in product.variants %}
            {% if variant.available %}
              <option
                data-vartitle="{{ variant.title }}"
                data-avaiablity="yes"
                data-text=""
                {% if variant.featured_media %}data-image="{{ variant.featured_media | img_url:'medium' }} {% else %} data-image="{{ product.featured_image | img_url: 'medium' }}{% endif %}" {% if variant == product.selected_or_first_available_variant %}
                                                        selected="selected" {% endif %}
                                    value="
                {{ variant.id }}
                ">
                {{ variant.title }} - {{ variant.price | money_with_currency }}
              </option>
            {% else %}
              <option
                data-vartitle="{{ variant.title }}"
                data-avaiablity="no"
                data-text="- Sold Out"
                {% if variant.featured_media %}data-image="{{ variant.featured_media | img_url:'medium' }} {% else %} data-image="{{ product.featured_image | img_url: 'medium' }}{% endif %}">
                {{ variant.title }} - {{ 'products.product.sold_out' | t }}
              </option>
            {% endif %}
          {% endfor %}
        </select>
      </div>
    </div>
  </div>
