<script>
  if(typeof(Spurit) === 'undefined'){
    var Spurit = {};
  }
  if(!Spurit.CountdownTimer){
    Spurit.CountdownTimer = {};
  }
  if(!Spurit.CountdownTimer.snippet){
    Spurit.CountdownTimer.snippet = {};
  }
  Spurit.CountdownTimer.snippet.shopHash = '67ec4f9158e1290b4ffff236a662942a';
  Spurit.CountdownTimer.snippet.cacheTimestamp = 1602652410;

  {% assign page = template | split: '.' | first %}
  {% if page == 'product' and product and product.id %}
    Spurit.CountdownTimer.snippet.productId = {{ product.id }};
  {% endif %}
  {% if page == 'collection' and collection and collection.id %}
  Spurit.CountdownTimer.snippet.collectionId = {{ collection.id }};
  {% endif %}
</script>

    <script lazy-src="https://cdn-spurit.com/shopify-apps/timer-app/common.js"></script>
    <link lazy-href="https://cdn-spurit.com/shopify-apps/timer-app/common.css" rel="stylesheet" type="text/css" media="all">
