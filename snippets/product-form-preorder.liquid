<script>
let currentVariant = "{{ product.selected_or_first_available_variant.title }}";

function getSecondPart(str) {
    return str.split(' / ')[1];
}

function getfirstPart(str) {
  return str.substring(0, str.indexOf(' '));
}

current = getSecondPart(currentVariant);
let newcounter = 0;
let x 
let fvar
let svar
let availability
jsons = "";
counter = 0;
{% for variant in product.variants %}
  svar = getSecondPart("{{variant.title}}")
  if(current == svar){
    counter++;
  }
{% endfor %}
<!-- 08 -->
{% for variant in product.variants %}

   x = "{{ variant.title }}";
   fvar = getfirstPart(x)
   svar = getSecondPart(x)
   if(current == svar){
      if(newcounter == 0){
          jsons =  jsons + '[';
      }
      availability = {{variant.available}}
      jsons = jsons + '{"firstVariant":"' + fvar + '","secondVariant":"' + svar + '","available":"' + availability + '"}';
      newcounter++;
      if(newcounter < counter){
          jsons =  jsons + ',';
      }else{
          jsons =  jsons + ']';
      }
   }

{% endfor %}
  
   jsons = JSON.parse(jsons);

 
</script>
