<script type="application/ld+json">{
  "@context": "https://schema.org",
  "@type": "BreadcrumbList",
  "itemListElement": [{
    "@type": "ListItem",
    "position": 1,
    "name": "Home",
    "item": "{{ shop.url }}"
  }
  
  {%- if request.page_type == 'page' -%}, {
    "@type": "ListItem",
    "position": 2,
    "name": "{{ page.title | strip_html | strip_newlines | strip }}",
    "item": "{{ shop.url }}{{ product.url }}"
  }
  {%- endif -%}

  {%- if request.page_type == 'product' -%}, {
    "@type": "ListItem",
    "position": 2,
    "name": "{{ product.title | strip_html | strip_newlines | strip | escape }}",
    "item": "{{ shop.url }}{{ product.url }}"
  }
  {%- endif -%}
  
  {%- if request.page_type == 'page' -%}, {
    "@type": "ListItem",
    "position": 2,
    "name": "{{ page.title | strip_html | strip_newlines | strip | escape }}",
    "item": "{{ shop.url }}{{ page.url }}"
  }
  {%- endif -%}
  
  {%- if request.page_type == 'policy' -%}, {
    "@type": "ListItem",
    "position": 2,
    "name": "{{ shop.privacy_policy.title | strip_html | strip_newlines | strip | escape }}",
    "item": "{{ shop.url }}{{ shop.privacy_policy.url }}"
  }
  {%- endif -%}
  
  {%- if request.page_type == 'collection' -%}, {
    "@type": "ListItem",
    "position": 2,
    "name": "{{ collection.title | strip_html | strip_newlines | strip | escape }}",
    "item": "{{ shop.url }}{{ collection.url }}"
  }
  {%- endif -%}
  
  {%- if request.page_type == 'blog' -%}, {
    "@type": "ListItem",
    "position": 2,
    "name": "{{ blog.title | strip_html | strip_newlines | strip | escape }}",
    "item": "{{ shop.url }}{{ blog.url }}"
  }
  {%- endif -%}
  
  {%- if request.page_type == 'article' -%}, {
    "@type": "ListItem",
    "position": 2,
    "name": "{{ blog.title | strip_html | strip_newlines | strip | escape }}",
    "item": "{{ shop.url }}{{ blog.url }}"
  }, {
    "@type": "ListItem",
    "position": 3,
    "name": "{{ article.title | strip_html | strip_newlines | strip | escape }}",
    "item": "{{ shop.url }}{{ article.url }}"
  }
  {%- endif -%}
  ]
}
</script>

{%- if request.page_type == 'collection' -%}
<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "CollectionPage",
  "name": "{{ page_title }}",
  "description": "{{ page_description | strip_html }}",
  "url": "{{ shop.url }}{{ collection.url }}",
  "image": "{{ shop.url }}{{ collection.featured_image | image_url }}",
  "mainEntity": {
    "@type": "ItemList",
    "itemListElement": [
      {% for product in collection.products %}
      {
        "@type": "ListItem",
        "position": "{{ forloop.index }}",
        "url": "{{ shop.url }}{{ product.url }}"
      }{% unless forloop.last %},{% endunless %}
      {% endfor %}
    ]
  }
}
</script>
{%- endif -%}

{%- if request.page_type == 'article' -%}
<script type="application/ld+json">{
    "@context": "https://schema.org/",
    "@type": "CreativeWorkSeries",
    "name": "{{ article.title }}",
    "aggregateRating": {
        "@type": "AggregateRating",
        "ratingValue": "5",
        "bestRating": "5",
        "ratingCount": "12"
    }
}</script>
{%- endif -%}
