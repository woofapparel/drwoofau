<script>
  var comprice = {}
  var normprice = {}
  {% for variant in product.variants %}
    var x = "{{ variant.compare_at_price }}"
    var y = "{{ variant.price }}"
    var a = "{{ variant.title }}"
    a = a.toLowerCase().replace(/[^a-z0-9]+/g, '-').replace(/-$/, '').replace(/^-/, '');
    comprice[a] = x
    normprice[a] = y
  {% endfor %}
