        "top_mode": true,
        "light_theme": false,
        "main_header": "5 Reasons Our Scrubs Will Make You the Talk of the Hospital (In a Good Way!)",
        "main_header_size": 40,
        "main_header_size_mobile": 32,
        "main_header_space": 40,
        "content": "Because saving lives is serious business, but your scrubs don't have to be!",
        "textaroundtop": "",
        "textaroundbottom": "",
        "button_text_1": "SHOP NOW",
        "button_1_url": "shopify:\/\/collections\/printed-pattern-scrub-tops",
        "button_text_2": "",
        "button_2_url": "",
        "header_image": "shopify:\/\/shop_images\/ATCVideo3-ezgif.com-optimize.gif",
        "mobile_image": "shopify:\/\/shop_images\/ATCVideo3-ezgif.com-optimize.gif",
